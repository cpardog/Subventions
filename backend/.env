NODE_ENV=development
BACKEND_PORT=4000

# Database
DATABASE_URL="postgresql://subvenciones:subvenciones_password@localhost:5432/subvenciones_db?schema=public"

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=dev-jwt-secret-change-in-production-12345
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# Session
SESSION_SECRET=dev-session-secret-change-in-production-12345

# CORS
CORS_ORIGIN=http://localhost:3001

# Upload
UPLOAD_PATH=./uploads
MAX_FILE_SIZE_MB=10

# MFA
MFA_ISSUER=SistemaSubvenciones

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
